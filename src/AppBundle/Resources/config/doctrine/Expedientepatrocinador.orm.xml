<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Expedientepatrocinador" table="expedientepatrocinador">
    <id name="idexppatrocinador" type="integer" column="IDEXPPATROCINADOR">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="docidentidadpatrocinador" type="string" column="DOCIDENTIDADPATROCINADOR" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecharegistropatrocinador" type="datetime" column="FECHAREGISTROPATROCINADOR" nullable="true"/>
    <field name="montoaportarpratocinador" type="decimal" column="MONTOAPORTARPRATOCINADOR" precision="8" scale="0" nullable="true"/>
    <field name="telefonopatrocinador" type="string" column="TELEFONOPATROCINADOR" length="11" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccionpatrocinador" type="string" column="DIRECCIONPATROCINADOR" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadopatrocinador" type="boolean" column="ESTADOPATROCINADOR" nullable="true"/>
  </entity>
</doctrine-mapping>
