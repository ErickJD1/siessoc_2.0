<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Role" table="role">
    <id name="idrole" type="string" column="IDROLE">
    </id>
    <field name="nomrole" type="string" column="NOMROLE" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcionrole" type="string" column="DESCRIPCIONROLE" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadorole" type="boolean" column="ESTADOROLE" nullable="true"/>
  </entity>
</doctrine-mapping>
